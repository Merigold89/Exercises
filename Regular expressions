import re                                                       # sprawdzanie emotikonek

string = [':)', ':-)', ':(', ':-(', ':o', ':-o']
template = r'[:o][-]?[:o)(]'
for emot in string:
    print(re.match(template, emot))

template = '[0-9a-fA-F][0-9a-fA-F]?'                            # liczby szestnastkowe
       
template = 'M[rs][.]? Smith'                                   # zwrot grzeczno≈õciowy Mr, Ms Mr., Ms.
